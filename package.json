    "dependencies": {
        "@prisma/client": "^6.3.1",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "joi": "^17.13.3"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.1",
        "jest": "^29.7.0",
        "jest-mock-extended": "^4.0.0-beta1",
        "nodemon": "^3.1.9",
        "prisma": "^6.3.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
